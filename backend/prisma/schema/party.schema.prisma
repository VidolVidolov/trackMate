model Party {
    id                Int       @id @default(autoincrement())
    name              String
    members           User[]
    startingLocation  Location? @relation("StartingLocation")
    endingLocation    Location? @relation("EndingLocation")
    owner             User      @relation("Owner", fields: [userId], references: [id])
    userId            Int       @unique
    timeCreated       DateTime
    timeClosed        DateTime?
    partyDismissed    Boolean   @default(false)
    distanceTravelled Decimal?
    timeTaken         Decimal?
    locationId        Int?
}
